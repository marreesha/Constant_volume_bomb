FoamFile
{
    version         2.0;
    format          ascii;
    location        "0";
    class           volScalarField;
    object          omega;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{

	inlet
    {
type            zeroGradient;
    }

    outlet
    {
type            zeroGradient;
    }

    wallDiffuser
    {
type            zeroGradient;
    }
}


